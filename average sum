/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
    public static int avrSum(int n, int a[]){
        int s=0;
        for(int i=0;i<n;i++){
        s=s+a[i];
      
        }
    int avr = s/n;
    
       return avr;
    
    }

	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
	//	int a = sc.nextInt();
	  //  int b = sc.nextInt();
	//	int c = sc.nextInt();
		int n = sc.nextInt();
	int a[]=new int[n];
	for(int i=0;i<n;i++)
	      a[i]=sc.nextInt();
	
		System.out.println(avrSum(n,a));
	}
}
